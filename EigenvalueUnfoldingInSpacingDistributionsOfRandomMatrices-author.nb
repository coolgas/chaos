      Hold[
       Row[{
         Manipulate`Place[2]}]], Manipulate`Dump`ThisIsNotAControl}}, 
    Typeset`size$$ = Automatic, Typeset`update$$ = 0, Typeset`initDone$$, 
    Typeset`skipInitDone$$ = False}, 
    DynamicBox[Manipulate`ManipulateBoxes[
     2, StandardForm, 
      "Variables" :> {$CellContext`dim$$ = 2500, $CellContext`ss$$ = 2}, 
      "ControllerVariables" :> {}, 
      "OtherVariables" :> {
       Typeset`show$$, Typeset`bookmarkList$$, Typeset`bookmarkMode$$, 
        Typeset`animator$$, Typeset`animvar$$, Typeset`name$$, 
        Typeset`specs$$, Typeset`size$$, Typeset`update$$, Typeset`initDone$$,
         Typeset`skipInitDone$$}, "Body" :> 
      Module[{$CellContext`evs1$, $CellContext`hdens1$, $CellContext`spacs1$, \
$CellContext`gaps1$, $CellContext`unfoldeigs$, $CellContext`hdens2$, \
$CellContext`spacs2$, $CellContext`ungaps$}, 
        SeedRandom[$CellContext`dim$$]; $CellContext`evs1$ = RandomVariate[
           MatrixPropertyDistribution[
            Eigenvalues[$CellContext`x], 
            Distributed[$CellContext`x, 
             
             GaussianOrthogonalMatrixDistribution[$CellContext`dim$$]]]]; \
$CellContext`hdens1$ = 
         Histogram[$CellContext`evs1$/Sqrt[2 $CellContext`dim$$], Automatic, 
           "PDF", Frame -> True, ChartStyle -> Orange, 
           PlotRange -> {{-1.2, 1.2}, All}, FrameLabel -> {
             Row[{"energy ", 
               Style["E", Italic], "/", 
               Sqrt[
                Row[{"2", 
                  Style["N", Italic]}]]}], 
             Row[{"density \[Rho](", 
               Style["E", Italic], ")"}]}, LabelStyle -> Directive[Black, 12],
            FrameTicksStyle -> 10, RotateLabel -> True, 
           ImagePadding -> {{Automatic, Automatic}, {Automatic, Automatic}}, 
           ImageSize -> {300, Automatic}]; $CellContext`spacs1$ = 
         Differences[
           Sort[$CellContext`evs1$]]; $CellContext`gaps1$ = 
         Histogram[$CellContext`spacs1$, Automatic, "PDF", ChartStyle -> Gray,
            Frame -> True, FrameLabel -> {
             Row[{"spacing ", 
               Style["s", Italic]}], 
             Row[{
               Style["P", Italic], "(", 
               Style["s", Italic], ")"}]}, LabelStyle -> Directive[Black, 12],
            FrameTicksStyle -> 10, RotateLabel -> True, PlotRange -> Full, 
           ImagePadding -> {{Automatic, 5}, {Automatic, 5}}, 
           ImageSize -> {300, 201}]; $CellContext`unfoldeigs$ = 
         Map[$CellContext`\[Eta][#, $CellContext`dim$$]& , \
$CellContext`evs1$]; $CellContext`hdens2$ = 
         Histogram[$CellContext`unfoldeigs$/$CellContext`dim$$, Automatic, 
           "PDF", Frame -> True, ChartStyle -> Yellow, FrameLabel -> {
             Row[{"energy ", 
               Style["E", Italic], "/", 
               Style["N", Italic]}], 
             Row[{"density \[Rho](", 
               Style["E", Italic], ")"}]}, LabelStyle -> Directive[Black, 12],
            FrameTicksStyle -> 10, RotateLabel -> True, 
           PlotRange -> {{0, 1}, {0, 1}}, 
           ImagePadding -> {{Automatic, Automatic}, {Automatic, Automatic}}, 
           ImageSize -> {300, 201}]; $CellContext`spacs2$ = Differences[
           Sort[$CellContext`unfoldeigs$]]; $CellContext`ungaps$ = 
         Histogram[$CellContext`spacs2$, {0, 4, 0.1}, "PDF", Frame -> True, 
           ChartStyle -> Green, FrameLabel -> {
             Row[{"spacing ", 
               Style["s", Italic]}], 
             Row[{
               Style["P", Italic], "(", 
               Style["s", Italic], ")"}]}, LabelStyle -> Directive[Black, 12],
            FrameTicksStyle -> 10, RotateLabel -> True, 
           PlotRange -> {{0, 4}, {0, 1}}, 
           ImagePadding -> {{Automatic, 5}, {Automatic, 5}}, Epilog -> Text[
             Style[
              StringJoin["\!\(\*OverscriptBox[\(s\), \(_\)]\) = ", 
               ToString[
                N[
                 Abs[
                  Mean[$CellContext`spacs2$]]]]], 12, Blue], {2.5, 0.7}]]; 
        If[$CellContext`ss$$ == 1, 
          Row[{
            Show[{$CellContext`hdens1$, 
              Plot[
               PDF[
                WignerSemicircleDistribution[
                1], $CellContext`x], {$CellContext`x, -1, 1}, 
               PlotStyle -> {Blue, 
                 Thickness[0.01]}]}, 
             ImageSize -> {300, 201}], $CellContext`gaps1$}], 
          Row[{$CellContext`hdens2$, 
            Show[{$CellContext`ungaps$, 
              Plot[
               $CellContext`WignerSurmisePDF[$CellContext`x, 
                1], {$CellContext`x, 0, 4}, PlotStyle -> {Blue, 
                 Thickness[0.01]}]}, ImageSize -> {300, 201}]}]]], 
      "Specifications" :> {{{$CellContext`dim$$, 2500, 
          Row[{"matrix dimension ", 
            Style["N", Italic]}]}, 500, 2500, 1, Appearance -> "Labeled", 
         ControlPlacement -> 1}, 
        Row[{
          Manipulate`Place[1]}], {{$CellContext`ss$$, 2, "show plots"}, {
         1 -> "before unfolding", 2 -> "after unfolding"}, ControlType -> 
         Setter, ControlPlacement -> 2}, 
        Row[{
          Manipulate`Place[2]}]}, 
      "Options" :> {
       SynchronousInitialization -> False, 
        TrackedSymbols :> {$CellContext`dim$$, $CellContext`ss$$}}, 
      "DefaultOptions" :> {ControllerLinking -> True}],
     ImageSizeCache->{643., {156., 162.}},
     SingleEvaluation->True],
    Deinitialization:>None,
    DynamicModuleValues:>{},
    Initialization:>(($CellContext`WignerSurmisePDF[
         Pattern[$CellContext`x, 
          Blank[]], 
         Pattern[$CellContext`\[Beta], 1]] := (Pi ($CellContext`x/2)) 
        Exp[(-Pi) ($CellContext`x/2)^2]; $CellContext`\[Eta][
         Pattern[E, 
          Blank[]], 
         Pattern[$CellContext`dim, 
          Blank[]]] := (1/(2 Pi)) (
         E Sqrt[2 $CellContext`dim - E^2] + (2 $CellContext`dim) 
          ArcSin[E/Sqrt[2 $CellContext`dim]] + Pi $CellContext`dim)); 
     Typeset`initDone$$ = True),
    SynchronousInitialization->False,
    UndoTrackedVariables:>{Typeset`show$$, Typeset`bookmarkMode$$},
    UnsavedVariables:>{Typeset`initDone$$},
    UntrackedVariables:>{Typeset`size$$}], "Manipulate",
   Deployed->True,
   StripOnInput->False],
  Manipulate`InterpretManipulate[1]]], "Output",
